@article{schmidIdeologyDiscourseProust1999,
  title = {Ideology and {{Discourse}} in {{Proust}}: The {{Making}} of "{{M}}. de {{Charlus}} Pendant La Guerre"},
  author = {Schmid, Marion},
  year = {1999},
  journal = {The Modern Language Review},
  volume = {94},
  number = {4},
  pages = {961--977},
  issn = {00267937},
  abstract = {In the preparatory manuscript drafts of 'M. de Charlus pendant la guerre', the first sketches for the war chapter show a Proust surprisingly lucid and outspoken about the political and intellectual climate in France after 1914, in particular about the growing nationalism and chauvinism amongst civilians, politicians, and journalists. Worried about a possible scandal, he considerably self-censored his text as "Le Temps retrouv\'e" was approaching publication. He defined a number of narrative strategies (character-focalization, meta-diegetic narration, conflicting discourses) that allowed him to distance himself from his criticism and thus to express his opinions in a more oblique and ironic way.}
}

