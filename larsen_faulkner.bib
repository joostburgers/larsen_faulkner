@article{mcbrideUtilityArtPolitics2000,
  title = {On the {{Utility}} of {{Art}} for {{Politics}}: Musil's "{{Armed Truce}} of {{Ideas}}"},
  author = {McBride, Patrizia C.},
  year = {2000},
  journal = {The German Quarterly},
  volume = {73},
  number = {4},
  pages = {366--86},
  issn = {00168831},
  abstract = {This article examines Robert Musil's paradoxical claim that an autonomous aesthetic sphere is the best ally of politics and concludes that Musil's articulation of a divide between art and politics provides an especially timely model for intellectual engagement today. For Musil the structural affinity shared by the political and the aesthetic is grounded in the way each facilitates an agonistic exchange between dissenting perspectives. This clash of perspectives is what averts the threat of totalitarianism, which Musil identifies with the imposition of a totalizing, absolute vision upon the disparate life spheres that characterize the modern world. This forced integration was fatefully realized in Musil's day by the strategy of Gleichschaltung pursued by the Nazi regime. Musil's position appears appealing today because it reaffirms the critical mandate of the intellectual and the artist while refusing to ground it in the claim to a superior vantage point.}
}

