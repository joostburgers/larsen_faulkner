@article{fraissePsychologieFoulesMarcel1988,
  title = {Psychologie Des Foules: Marcel {{Proust}} Lecteur de {{Gustave Le Bon}}},
  author = {Fraisse, Luc},
  year = {1988},
  journal = {Revue d'Histoire litt\'eraire de la France},
  volume = {88},
  number = {1},
  pages = {82--96},
  issn = {00352411},
  abstract = {Une lettre r\'ecemment publi\'ee de Marcel Proust en 1899, faisant allusion au livre du sociologue Gustave Le Bon, Psychologie des foules, montre le futur auteur de la Recherche en train d'\'elaborer sa doctrine sociologique en pleine affaire Dreyfus. On peut reconstituer l'influence de Le Bon sur l'\'evocation des foules dans Jean Santeuil, alors en cours de r\'edaction, et \`a long terme sur les situations romanesques du Temps perdu. Toutefois Proust visiblement fait allusion \`a Le Bon en pensant \`a la sociologie de Tarde, et un faisceau de comparaisons entre les deux sociologues s'ouvre, mis \`a son tour en parall\`ele avec l'univers romanesque de Proust. Enfin, une petite publication de Freud en 1921 consacr\'ee \`a Le Bon permet de pr\'eciser, \`a propos de la contagion qui cimente les foules, la parent\'e unissant Proust et Freud sur l'analyse des m\'ecanismes du r\^eve comme \'equivalent du processus de la cr\'eation litt\'eraire. La critique de Freud marque aussi les limites de Le Bon : \`a ses foules de rue et de hasard, Proust pr\'ef\'erera les soci\'et\'es, dont la lente \'evolution est r\'egie par les lois et figure commod\'ement la cr\'eation d'une oeuvre en devenir.}
}

