@article{randelParenthesesFaulknerAbsalom1971,
  title = {Parentheses in {{Faulkner}}'s {{{\emph{Absalom}}}}{\emph{, }}{{{\emph{Absalom}}}}{\emph{!}}},
  author = {Randel, Fred V.},
  year = {1971},
  journal = {Style},
  volume = {5},
  number = {1},
  pages = {70--87},
  publisher = {{Penn State University Press}},
  issn = {0039-4238},
  abstract = {Absalom, Absalom! treats on individual and social levels the passage from a world of dream to a world of limits. Four aspects of this theme are related to types of parentheses which recur in the novel. The problem of the self and external world is embodied in the interpretative digression, the parenthesis which reveals Quentin Compson's unspoken imaginings, and the interruption by the external world in the person of Shreve. The problem of communication is adumbrated by the many explanatory parentheses, including the parenthesized noun in apposition with a preceding pronoun. The problem of history is revealed not only in the allusion to another level of time, but also in the parenthesis which casts doubt on the matter outside the parenthesis. The difficulty of encompassing reality within a single concept or mind is dramatized by parentheses of confrontation and tension. The distribution of parentheses throughout the novel reveals a peculiar importance to the middle chapter, and this uniqueness is explained as centering the novel on Quentin's point of contact with the world of limits.},
  file = {C\:\\Users\\joost\\Zotero\\storage\\H5GAQP56\\Randel - 1971 - PARENTHESES IN FAULKNER'S ABSALOM, ABSALOM!.pdf}
}

