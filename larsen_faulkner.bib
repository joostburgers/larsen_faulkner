@article{gilmanSalomeSyphilisSarah1993,
  title = {Salome, {{Syphilis}}, {{Sarah Bernhardt}} and the "{{Modern Jewess}}"},
  shorttitle = {Salome},
  author = {Gilman, Sander L.},
  year = {1993},
  journal = {The German Quarterly},
  volume = {66},
  number = {2},
  pages = {195--211},
  issn = {00168831},
  abstract = {It is a commonplace that the image of Salome haunted the fin de si\`ecle fantasy. Yet the anxiety that Salome represented was evoked by both the stereotype of the "woman" and that of the "Jew"-two categories of organization seemingly at odds during the period. In examining a series of texts from the turn of the century, ranging from Oskar Panizza's drama The Council of Love to a satire on Sarah Bernhardt, the ever-shifting image of Salome can be seen to evoke aspects of both images. This complexity is also found in the image of the "modern Jewess" in a text by a Jewish woman writer at the turn of the century and its psychoanalytic analogy. The internalization of images of difference in the image of the "modern Jewess" can present a test case for the power of such images at that time.}
}

